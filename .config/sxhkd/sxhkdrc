super + Return
    $TERMINAL

super + a
    automount

super + shift + a
    unmount

super + b
    $BROWSER

super + shift + b
    $BROWSER -private-window

super + d
    dmenu_run

super + shift + e
    lock

super + m
    minecraft-launcher

super + t
    tor-browser

super + v
    virtualbox

super + shift + v
    vlc

XF86MonBrightnessDown
	xbacklight -dec 10

XF86MonBrightnessUp
	xbacklight -inc 10

XF86Audio{Raise,Lower}Volume
    pactl set-sink-volume 0 {+,-}5%; refbar

XF86AudioMute
    pactl set-sink-mute 0 toggle; refbar
